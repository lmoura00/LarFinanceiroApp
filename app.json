{
  "expo": {
    "name": "Lar Financeiro",
    "slug": "LarFinanceiroApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon-dark.png",
    "scheme": "larfinanceiroapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lsm-00.LarFinanceiroApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon-dark.png",
        "backgroundColor": "#222222"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lsm00.LarFinanceiroApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
